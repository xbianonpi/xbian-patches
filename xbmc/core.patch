-    // don't start caching if it's only a single stream that has run dry
-    if(m_dvdPlayerAudio.GetLevel() > 50
-    || m_dvdPlayerVideo.GetLevel() > 50)
-      return false;
-
-    if(current.inited)
-      SetCaching(CACHESTATE_FULL);
-    else
-      SetCaching(CACHESTATE_INIT);
-    return true;
-  }
-  return false;
-}
