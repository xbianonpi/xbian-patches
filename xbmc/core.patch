-    if (IsBetterStream(m_CurrentTeletext, pStream)) OpenTeletextStream(pStream->iId, pStream->source);
-
-    // process the packet
-    ProcessPacket(pStream, pPacket);
-
-    // check if in a cut or commercial break that should be automatically skipped
-    CheckAutoSceneSkip();
-  }
-}
